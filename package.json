{
    "name": "advent-of-code-2024-calendar",
    "private": false,
    "version": "0.0.1",
    "description": "Advent of Code 2024 calendar highlighting solutions to challenge problems",
    "author": "Jackson Reeves",
    "type": "module",
    "license": "MIT",
    "scripts": {
        "build": "tsc && vite build",
        "check": "npm run build && npm run lint && npm run format && echo '\nAll checks passed!'",
        "dev": "vite --open",
        "format": "prettier . --write",
        "lint": "eslint --fix",
        "relock": "rm -rf node_modules && rm package-lock.json && npm i"
    },
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.14.1"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.22.9",
        "@babel/preset-react": "^7.22.5",
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "@typescript-eslint/eslint-plugin": "^6.10.0",
        "@typescript-eslint/parser": "^6.10.0",
        "@vitejs/plugin-react-swc": "^3.3.2",
        "@vitejs/plugin-react": "^4.2.0",
        "eslint": "^8.53.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-check-file": "^2.5.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.4",
        "eslint-plugin-require-explicit-generics": "^0.4.2",
        "prettier": "^3.0.0",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.2.2",
        "vite": "^5.0.0"
    }
}
